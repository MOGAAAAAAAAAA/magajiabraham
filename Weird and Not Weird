User_number=input()
if (User_number % 2 == 0 and User_number >= 2 and User_number <= 5):
    print("Not Weird")
elif (User_number % 2 == 0 and User_number >= 6 and User_number <= 20):
    print("Weird")
elif (User_number % 2 == 0 and User_number > 20):
    print("Not Weird")
else:
    print("Weird")
